#include<stdio.h>
int main()
{
    int n1,n2,n3,n4,n5,per;
    printf("Enter the  number : ");
    scanf("%d %d %d %d %d",&n1,&n2,&n3,&n4,&n5);
    per=(n1+n2+n3+n4+n5)*100/500;
    if(per>=60)
    printf("first division ! ");
    if((per>=50)&&(per<=59))
    printf("second division !");
    if((per>=40)&&(per<=49))
    printf("Third division !");
    if(per<40)
    printf(" yor are failed ! ");
    return 0;
}